{
  "name": "boilerplate-v3",
  "version": "3.0.0",
  "description": "Boilerplate Node.js API",
  "main": "index.js",
  "engines": {
    "npm": ">=8.13.2",
    "node": ">=20.10.0"
  },
  "peerDependencies": {
    "eslint": "^7 || ^8"
  },
  "scripts": {
    "dev": "cross-env ENV_FILE=./src/config/env/.env.dev nodemon",
    "staging": "cross-env ENV_FILE=./src/config/env/.env.staging ts-node-dev --poll --respawn --transpile-only ./src/index.ts",
    "migration:make": "knex migrate:make --knexfile ./src/database/knexfile.ts",
    "lint:check": "eslint --ext ts --report-unused-disable-directives .",
    "test": "cross-env ENV_FILE=./src/config/env/.env.test jest --detectOpenHandles"
  },
  "author": "Jan Libal",
  "license": "ISC",
  "devDependencies": {
    "@strv/eslint-config-base": "^3.5.0",
    "@strv/eslint-config-node": "^3.3.2",
    "@strv/eslint-config-typescript": "^4.1.2",
    "@types/dotenv-parse-variables": "^2.0.3",
    "@types/jest": "^29.5.11",
    "@types/koa__cors": "^5.0.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-compress": "^4.0.6",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-router": "^7.4.8",
    "@types/lodash": "^4.14.202",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "pino-pretty": "^10.3.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "cross-env": "^7.0.3",
    "dotenv-extended": "^2.9.0",
    "dotenv-parse-variables": "^2.0.0",
    "jsonschema": "^1.4.1",
    "koa": "^2.15.0",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-compress": "^5.1.1",
    "koa-helmet": "^7.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.1",
    "lodash": "^4.17.21",
    "objection": "^3.1.3",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "@strv/node/v20",
      "@strv/node/optional",
      "@strv/node/style",
      "@strv/mocha"
    ]
  }
}
